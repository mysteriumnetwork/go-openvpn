[ -z "$DEP_DIR" ] && export DEP_DIR=$HOME/src/android
export PLATFORM=android-a8a
export ABI=arm64-v8a
export DEBUG_BUILD=0
export OTHER_COMPILER_FLAGS=""
export CXX_COMPILER_FLAGS="-std=c++1y"
export LIB_OPT_LEVEL="-O3"
export LIB_FPIC="-fPIC"
export TC=/usr/$ANDROID_CHAIN_ARM64
export PLATFORM_FLAGS="-D__ANDROID_API__=28 -march=armv8-a -fomit-frame-pointer"
export GPP_CMD="aarch64-linux-android28-clang++"
export GCC_CMD="aarch64-linux-android28-clang"
export AR_CMD=aarch64-linux-android-ar
export RANLIB_CMD=aarch64-linux-android-ranlib
